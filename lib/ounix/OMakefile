static. =
	CAMLLIB=$(shell ocamlc -where)

.PHONY: clean

INCLUDES += $(CAMLLIB)
UNIX_CLIB = $(StaticCLibrary ounix_stubs, ounix_stubs pty_stubs)

FILES[] =
   olog
   ounix

PROGRAM = ounix

OCAML_CLIBS += ounix_stubs

.DEFAULT: $(OCamlLibrary $(PROGRAM),$(FILES)) $(UNIX_CLIB)

clean:
	$(CLEAN) $(PROGRAM) dllounix_stubs.so
